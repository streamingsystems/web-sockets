{
    "name": "streamingsystems/web-sockets",
    "description": "An easy to use WebSocket server",
    "keywords": [
        "streamingsystems",
        "web-sockets"
    ],
    "homepage": "https://github.com/streamingsystems/web-sockets",
    "license": "MIT",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/streamingsystems/Ratchet.git"
        }
    ],
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "marcel@beyondco.de",
            "homepage": "https://beyondcode.de",
            "role": "Developer"
        },
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "cboden/ratchet": "dev-master",
        "facade/ignition-contracts": "^1.0",
        "guzzlehttp/psr7": "^1.7|^2.0",
        "illuminate/broadcasting": "^11.0",
        "illuminate/console": "^11.0",
        "illuminate/http": "^11.0",
        "illuminate/routing": "^11.0",
        "illuminate/support": "^11.0",
        "pusher/pusher-php-server": "^7.0",
        "react/dns": "^1.1",
        "react/http": "^1.1",
        "symfony/http-kernel": "^7.0",
        "symfony/psr-http-message-bridge": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "BeyondCode\\LaravelWebSockets\\": "src"
        }
    },
    "autoload-dev": {

    },
    "scripts": {

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "BeyondCode\\LaravelWebSockets\\WebSocketsServiceProvider"
            ],
            "aliases": {
                "WebSocketRouter": "BeyondCode\\LaravelWebSockets\\Facades\\WebSocketRouter"
            }
        }
    }
}
